    {
      "key": "mem_util",
      "title": "Memory Utilization",
      "unit": "%",
      "min": 0,
      "max": 100,
      "read_rate": 1,
      "formula": "float(r.get('used_mem')) / float(r.get('total_mem')) * 100",
      "icon": "0xf035b"
    },
    {
      "key": "mem_used",
      "title": "Used Memory",
      "unit": "GB",
      "min": 0,
      "max": 2048,
      "read_rate": 1,
      "redis_key": "used_mem",
      "icon": "0xf035b",
      "label": "USED"
    },
    {
      "key": "mem_free",
      "title": "Free Memory",
      "unit": "GB",
      "min": 0,
      "max": 2048,
      "read_rate": 1,
      "redis_key": "avail_mem",
      "icon": "0xf035b",
      "label": "FREE"
    }
