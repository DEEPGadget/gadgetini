  "sensor_templates": [
    {
      "key": "gpu{i}_temp",
      "count": "gpu_count",
      "title": "GPU{i} Temp",
      "unit": "°C",
      "min": 10,
      "max": 120,
      "read_rate": 1,
      "redis_key": "gpu{i}_gpu_temp",
      "icon": "0xf0510",
      "label": "G{i}"
    },
    {
      "key": "gpu{i}_power",
      "count": "gpu_count",
      "title": "GPU{i} Power",
      "unit": "W",
      "min": 0,
      "max": 700,
      "read_rate": 1,
      "redis_key": "gpu{i}_gpu_power",
      "icon": "0xf140b",
      "label": "G{i}"
    },
    {
      "key": "cpu{i}_temp",
      "count": "cpu_count",
      "title": "CPU{i} Temperature",
      "unit": "°C",
      "min": 10,
      "max": 120,
      "read_rate": 1,
      "redis_key": "cpu_{i}_temp",
      "icon": "0xf0510",
      "label": "CPU{i}"
    },
    {
      "key": "cpu{i}_util",
      "count": "cpu_count",
      "title": "CPU{i} Utilization",
      "unit": "%",
      "min": 0,
      "max": 100,
      "read_rate": 1,
      "redis_key": "cpu_{i}_util",
      "icon": "0xf0ee0",
      "label": "CPU{i}"
    }
