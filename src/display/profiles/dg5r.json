            255,
            50,
            200
          ]
        ],
        "labels": [
          "IN1",
          "OUT1",
          "IN2",
          "OUT2"
        ]
      }
    },
    {
      "key": "gpu_daily",
      "type": "DailyViewer",
      "expand": "gpu_count",
      "params": {
        "title": "GPU Temp 24h",
        "sensor_keys": "gpu{i}_temp",
        "colors": "$GPU_COLORS",
        "labels": "G{i}"
      }
    },
    {
      "key": "cpu_daily",
