  "viewers": [
    {
      "key": "coolant",
      "type": "MultiSensorViewer",
      "params": {
        "title": "Coolant Overview",
        "sensor_keys": [
          "coolant_inlet1",
          "coolant_outlet1",
          "coolant_inlet2",
          "coolant_outlet2"
        ],
        "colors": [
          [
            0,
            200,
            255
          ],
          [
            255,
            140,
            0
          ],
          [
            0,
            220,
            100
          ],
          [
            255,
            50,
            200
          ]
        ],
        "labels": [
          "IN1",
          "OUT1",
          "IN2",
          "OUT2"
        ]
      }
    },
    {
      "key": "coolant_detail",
      "type": "CoolantDetailViewer",
      "params": {
        "loops": [
          {
            "title": "Loop 1",
            "sensor_keys": [
              "coolant_inlet1",
              "coolant_outlet1"
            ],
            "delta_key": "coolant_delta1",
            "colors": [
              [
                0,
                200,
                255
              ],
              [
                255,
                140,
                0
              ]
            ],
            "labels": [
              "IN1",
              "OUT1"
            ]
          },
          {
            "title": "Loop 2",
            "sensor_keys": [
              "coolant_inlet2",
              "coolant_outlet2"
            ],
            "delta_key": "coolant_delta2",
            "colors": [
              [
                0,
                220,
                100
              ],
              [
                255,
                50,
                200
              ]
            ],
            "labels": [
              "IN2",
              "OUT2"
            ]
          }
        ]
      }
    },
    {
      "key": "chassis",
      "type": "DualSensorViewer",
      "params": {
        "panels": [
          {
            "title": "Air Temperature",
            "sensor_key": "chassis_temp"
          },
          {
            "title": "Humidity",
            "sensor_key": "chassis_humid"
          }
        ]
      }
    },
    {
      "key": "gpu",
      "type": "TempUtilViewer",
      "expand": "gpu_count",
      "params": {
        "temp_title": "GPU Temperature",
        "util_title": "GPU Power",
        "sensor_keys": "gpu{i}_temp",
        "colors": "$GPU_COLORS",
        "labels": "G{i}",
        "util_keys": "gpu{i}_power"
      }
    },
    {
      "key": "cpu",
      "type": "TempUtilViewer",
      "expand": "cpu_count",
      "params": {
        "temp_title": "CPU Temperature",
        "util_title": "CPU Utilization",
        "sensor_keys": "cpu{i}_temp",
        "colors": "$CPU_COLORS",
        "labels": "CPU{i}",
        "util_keys": "cpu{i}_util"
      }
    },
    {
      "key": "memory",
      "type": "SensorViewer",
      "params": {
        "title": "MEM Info",
        "sensor_key": "mem_util",
        "sub1_key": "mem_used",
        "sub2_key": "mem_free",
        "fixed_min": 0,
        "fixed_max": 100,
        "sub1_autoscale": true,
        "sub2_autoscale": true
      }
    },
    {
      "key": "coolant_daily",
      "type": "DailyViewer",
      "params": {
        "title": "Coolant 24h",
        "sensor_keys": [
          "coolant_inlet1",
          "coolant_outlet1",
          "coolant_inlet2",
          "coolant_outlet2"
